<Window x:Class="Oef05.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ListBox HorizontalAlignment="Left" Height="299" Margin="260,10,0,0" VerticalAlignment="Top" Width="247" Name="stringListBox" SelectionChanged="stringListBox_SelectionChanged"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="245" Name="StringTextBox"/>
        <Button Content="Zoeken" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" Width="245" Name="searchButton" Click="ClickHandler"/>
        <Button Content="Verwijderen" HorizontalAlignment="Left" Margin="10,88,0,0" VerticalAlignment="Top" Width="245" Name="deleteButton" Click="ClickHandler"/>
        <Button Content="Toevoegen" HorizontalAlignment="Left" Margin="10,63,0,0" VerticalAlignment="Top" Width="245" Name="addButton" Click="ClickHandler"/>

    </Grid>
</Window>
